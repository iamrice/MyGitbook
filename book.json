{
    "title": "Born To Fire",
    "author": "TAIU",
    "description": "动感地带",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "-search",
        "expandable-chapters-small",
        "anchors",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon-absolute",
        "popup",
        "disqus"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "realhiphop"
        },
        "insert-logo": {
            "url": "images/微信图片_20201004195028.jpg",
            "style": "background:none;height: 80px;border-radius:10%;"
        },
        "github": {
            "url": "https://github.com/iamrice"
        },
        "github-buttons": {
            "buttons": [{
                "user": "Blankj",
                "repo": "glory",
                "type": "star",
                "size": "small",
                "count": true
            }]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon-absolute": {
            "favicon": "/images/favicon.ico",
            "appleTouchIconPrecomposed152": "/images/QQ图片20201112112441.png"
        }
